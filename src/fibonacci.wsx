def fibonacci(n)
    # Return the nth Fibonacci number.
    if n <= 0
        return 0
        end
    if n == 1
        return 1
        end
    if n >= 2
        a = 0
        b = 1
        for i in range(2, n + 1)
            a = b
            b = a + b
            end
        return b
        end
end